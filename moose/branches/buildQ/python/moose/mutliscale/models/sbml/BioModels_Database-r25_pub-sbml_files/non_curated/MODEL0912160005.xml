    <listOfRules>
      <rateRule metaid="metaid_0000034" variable="NFATP_cyt">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <minus/>
            <ci> Jexport </ci>
            <ci> Jdephosphorylation </ci>
          </apply>
        </math>
      </rateRule>
      <rateRule metaid="metaid_0000035" variable="NFAT_cyt">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <minus/>
            <ci> Jdephosphorylation </ci>
            <ci> Jtranslocate </ci>
          </apply>
        </math>
      </rateRule>
      <rateRule metaid="metaid_0000036" variable="NFAT_nuc">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <minus/>
            <ci> Jtranslocate </ci>
            <ci> Jexport </ci>
          </apply>
        </math>
      </rateRule>
      <assignmentRule metaid="metaid_0000025" variable="stim_on">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <piecewise>
            <piece>
              <cn type="integer"> 1 </cn>
              <apply>
                <and/>
                <apply>
                  <geq/>
                  <ci> time_model </ci>
                  <ci> time_before_stim </ci>
                </apply>
                <apply>
                  <leq/>
                  <apply>
                    <minus/>
                    <apply>
                      <minus/>
                      <ci> time_model </ci>
                      <ci> time_before_stim </ci>
                    </apply>
                    <apply>
                      <times/>
                      <apply>
                        <floor/>
                        <apply>
                          <divide/>
                          <apply>
                            <minus/>
                            <ci> time_model </ci>
                            <ci> time_before_stim </ci>
                          </apply>
                          <ci> stim_wavelength </ci>
                        </apply>
                      </apply>
                      <ci> stim_wavelength </ci>
                    </apply>
                  </apply>
                  <ci> stim_duration </ci>
                </apply>
              </apply>
            </piece>
            <otherwise>
              <cn type="integer"> 0 </cn>
            </otherwise>
          </piecewise>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000026" variable="k1">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <piecewise>
            <piece>
              <ci> k1_stim </ci>
              <apply>
                <eq/>
                <ci> stim_on </ci>
                <cn> 1 </cn>
              </apply>
            </piece>
            <otherwise>
              <ci> k1_unstim </ci>
            </otherwise>
          </piecewise>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000027" variable="Jdephosphorylation">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> k1 </ci>
              <ci> NFATP_cyt </ci>
            </apply>
            <apply>
              <times/>
              <ci> k2 </ci>
              <ci> NFAT_cyt </ci>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000028" variable="Jtranslocate">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <times/>
            <ci> k3 </ci>
            <ci> NFAT_cyt </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000029" variable="Jexport">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <times/>
            <ci> k4 </ci>
            <ci> NFAT_nuc </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000030" variable="NFAT_tot">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <plus/>
            <ci> NFATP_cyt </ci>
            <ci> NFAT_cyt </ci>
            <ci> NFAT_nuc </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000031" variable="percentage_NFAT_cyt">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <divide/>
            <apply>
              <times/>
              <ci> NFAT_cyt </ci>
              <cn> 100 </cn>
            </apply>
            <ci> NFAT_tot </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000032" variable="percentage_NFATP_cyt">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <divide/>
            <apply>
              <times/>
              <ci> NFATP_cyt </ci>
              <cn> 100 </cn>
            </apply>
            <ci> NFAT_tot </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="metaid_0000033" variable="percentage_NFAT_nuc">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <divide/>
            <apply>
              <times/>
              <ci> NFAT_nuc </ci>
              <cn> 100 </cn>
            </apply>
            <ci> NFAT_tot </ci>
          </apply>
        </math>
      </assignmentRule>
    </listOfRules>
  </model>
