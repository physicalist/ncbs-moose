    <listOfParameters>
      <parameter id="time_model" name="time" metaid="metaid_0000003"/>
      <parameter id="NFATP_cyt" constant="false" name="NFATP_cyt" metaid="metaid_0000004" value="5000"/>
      <parameter id="NFAT_cyt" constant="false" name="NFAT_cyt" metaid="metaid_0000005" value="0"/>
      <parameter id="NFAT_nuc" constant="false" name="NFAT_nuc" metaid="metaid_0000006" value="0"/>
      <parameter id="NFAT_tot" constant="false" name="NFAT_tot" metaid="metaid_0000007"/>
      <parameter id="k1_unstim" name="k1_unstim" metaid="metaid_0000008" value="0"/>
      <parameter id="k1_stim" name="k1_stim" metaid="metaid_0000009" value="0.359"/>
      <parameter id="k1" constant="false" name="k1" metaid="metaid_0000010"/>
      <parameter id="k2" name="k2" metaid="metaid_0000011" value="0.147"/>
      <parameter id="k3" name="k3" metaid="metaid_0000012" value="0.06"/>
      <parameter id="k4" name="k4" metaid="metaid_0000013" value="0.035"/>
      <parameter id="stim_wavelength" name="stim_wavelength" metaid="metaid_0000014" value="3"/>
      <parameter id="stim_duration" name="stim_duration" metaid="metaid_0000015" value="0.5"/>
      <parameter id="stim_on" constant="false" name="stim_on" metaid="metaid_0000016"/>
      <parameter id="time_before_stim" name="time_before_stim" metaid="metaid_0000017" value="1"/>
      <parameter id="Jdephosphorylation" constant="false" name="Jdephosphorylation" metaid="metaid_0000018"/>
      <parameter id="Jtranslocate" constant="false" name="Jtranslocate" metaid="metaid_0000019"/>
      <parameter id="Jexport" constant="false" name="Jexport" metaid="metaid_0000020"/>
      <parameter id="percentage_NFAT_cyt" constant="false" name="percentage_NFAT_cyt" metaid="metaid_0000021"/>
      <parameter id="percentage_NFATP_cyt" constant="false" name="percentage_NFATP_cyt" metaid="metaid_0000022"/>
      <parameter id="percentage_NFAT_nuc" constant="false" name="percentage_NFAT_nuc" metaid="metaid_0000023"/>
