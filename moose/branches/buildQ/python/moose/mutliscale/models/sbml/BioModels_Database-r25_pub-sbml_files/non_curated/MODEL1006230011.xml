    <listOfRules>
      <rateRule metaid="_217826" variable="phi_I">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <minus/>
            <apply>
              <plus/>
              <apply>
                <times/>
                <ci> alpha </ci>
                <ci> K_T </ci>
