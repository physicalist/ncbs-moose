              <ci> d1 </ci>
              <ci> phi_I </ci>
            </apply>
          </apply>
        </math>
      </rateRule>
      <rateRule metaid="_217829" variable="phi_R">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <minus/>
            <apply>
              <plus/>
              <apply>
                <times/>
                <apply>
                  <minus/>
                  <cn> 1 </cn>
                  <ci> alpha </ci>
                </apply>
                <ci> K_T </ci>
              </apply>
              <apply>
                <times/>
                <ci> k1 </ci>
                <ci> k2 </ci>
                <ci> phi_R </ci>
                <apply>
                  <minus/>
                  <cn> 1 </cn>
                  <apply>
                    <times/>
                    <ci> k3 </ci>
                    <apply>
                      <plus/>
                      <ci> phi_I </ci>
                      <ci> phi_R </ci>
                    </apply>
                  </apply>
                </apply>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci> d1 </ci>
              <ci> phi_R </ci>
            </apply>
          </apply>
        </math>
      </rateRule>
      <rateRule metaid="_217830" variable="T">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> k4 </ci>
              <ci> phi_I </ci>
            </apply>
            <apply>
              <times/>
              <ci> d2 </ci>
              <ci> T </ci>
            </apply>
          </apply>
        </math>
      </rateRule>
      <assignmentRule metaid="_217833" variable="total_macrophages">
        <math xmlns="http://www.w3.org/1998/Math/MathML">        
