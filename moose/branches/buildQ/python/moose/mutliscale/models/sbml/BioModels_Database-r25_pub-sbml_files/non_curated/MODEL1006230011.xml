                <cn> 3 </cn>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci> tau2 </ci>
              <apply>
                <power/>
                <ci> T </ci>
                <cn> 2 </cn>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci> tau3 </ci>
              <ci> T </ci>
            </apply>
            <ci> tau4 </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule metaid="_217837" variable="alpha">
