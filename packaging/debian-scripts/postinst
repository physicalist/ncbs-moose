#!/bin/sh
# This script is auto-generated by cmake. Don't modify it here
set -e
mkdir -p /etc/moose
mkdir -p /etc/moogli

case "$1" in
    configure)
        ( cd /tmp && python setup.py install --record /etc/moose/installed_files.txt  )       
        ( cd /tmp && python setup-moogli.py install --record /etc/moogli/installed_files.txt )
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac
exit 0
